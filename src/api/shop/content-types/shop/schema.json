{
  "kind": "collectionType",
  "collectionName": "shops",
  "info": {
    "singularName": "shop",
    "pluralName": "shops",
    "displayName": "Shop"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "shop"
    },
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "phone": {
      "type": "string"
    },
    "links": {
      "type": "component",
      "component": "contact.social-links",
      "repeatable": true
    },
    "location": {
      "type": "component",
      "component": "geo.location",
      "repeatable": false
    },
    "openingHours": {
      "type": "component",
      "component": "time.opening-hour",
      "repeatable": true
    }
  }
}
