{
  "kind": "collectionType",
  "collectionName": "opening_hours",
  "info": {
    "singularName": "opening-hour",
    "pluralName": "opening-hours",
    "displayName": "OpeningHour"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "start": {
      "type": "time"
    },
    "end": {
      "type": "time"
    },
    "day": {
      "type": "enumeration",
      "enum": [
        "mon",
        "tue",
        "wed",
        "thu",
        "fri",
        "sat",
        "sun"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "openingHours"
    }
  }
}
