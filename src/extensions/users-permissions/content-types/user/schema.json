{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "appleSub": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "refreshToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "default": "guest",
      "enum": [
        "shop",
        "artist",
        "guest"
      ]
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "city": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "contactName": {
      "type": "string"
    },
    "experience": {
      "type": "integer"
    },
    "phone": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "conditions": {
        "visible": {
          "!=": [
            {
              "var": "type"
            },
            "shop"
          ]
        }
      },
      "inversedBy": "childs"
    },
    "childs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "parent"
    },
    "openingHours": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::opening-hour.opening-hour",
      "mappedBy": "user"
    },
    "device_tokens": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::device-token.device-token",
      "mappedBy": "user"
    },
    "stripeAccountID": {
      "type": "string"
    },
    "payoutsEnabled": {
      "type": "boolean",
      "default": false
    },
    "verified": {
      "type": "boolean",
      "default": false
    },
    "depositAmount": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "payoutsEnabled"
            },
            true
          ]
        }
      },
      "required": false,
      "default": 10000,
      "min": 0
    },
    "portfolios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::portfolio.portfolio",
      "mappedBy": "owner"
    },
    "chargeDeposit": {
      "type": "boolean",
      "default": false
    }
  }
}
